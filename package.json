{
  "name": "react-dynamic-filterbar",
  "version": "0.1.2",
  "description": "A filter bar to host dynamic filters using react.",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.2",
    "@types/react-datepicker": "^2.2.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-select": "^2.0.15",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-flexbox-table": "^0.3.3",
    "react-hot-loader": "^4.6.5",
    "react-svg-icon-host": "^1.0.3",
    "svg-icon-converter": "^1.1.2",
    "typescript": "^3.4.1"
  },
  "watch": {
    "build:js": "src/{*.tsx, *.ts}",
    "build:css": "src/{*.scss, *.ts}"
  },
  "scripts": {
    "start": "npx parcel example/src/index.html --out-dir example/dist --open",
    "build": "npm run build:icons && npm run build:js && npm run build:css",
    "build:js": "npx tsc",
    "build:css": "npx node-sass src/scss/filter-bar.scss -o ./lib/css",
    "build:icons": "npx svg-convert -s icons -o src/filter-bar-icons -l FilterBarIcons",
    "clean": "rm -Rf ./lib",
    "watch": "npx npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clinta74/react-filter-bar.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clinta74/react-filter-bar/issues"
  },
  "homepage": "https://github.com/clinta74/react-filter-bar#readme",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "npm-watch": "^0.6.0",
    "react-datepicker": "^2.3.0",
    "react-select": "^2.4.2"
  }
}
